{
  "name": "@castframework/oracles",
  "version": "0.8.3",
  "description": "CAST oracles",
  "author": "Société Générale Forge",
  "license": "See license in LICENSE",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run generate:sdl && npm run build:app && npm run build:tests",
    "build:app": "$(npm bin)/tsc -b",
    "build:tests": "$(npm bin)/tsc --noEmit -p tsconfig.lint.json",
    "clean": "$(npm bin)/rimraf dist tsconfig.tsbuildinfo",
    "sonar": "$(npm bin)/sonar-scanner",
    "start:nodemon": "$(npm bin)/nodemon --config nodemon.json",
    "start": "(. ./bin/env-from-network && npm run start:nodemon )",
    "start:debug": "$(npm bin)/nodemon --config nodemon.debug.json",
    "stats": "nyc npm run test",
    "generate:sdl": "mkdir -p ./schema && node -r ts-node/register genSDLForModules.ts -o ./schema -i ./src/modules",
    "lint": "$(npm bin)/eslint --ignore-path .gitignore --ext ts .",
    "lint:fix": "$(npm bin)/eslint --fix --ignore-path .gitignore --ext ts .",
    "migration:generate": "$(npm bin)/ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
    "migration:revert": "$(npm bin)/ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "migration:create": "$(npm bin)/ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
    "pre-commit": "lint-staged --verbose",
    "schema:drop": "$(npm bin)/ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
    "test": "npm run test:unit",
    "test:unit": "$(npm bin)/ts-mocha tests/**/*.spec.ts",
    "printenv": "printenv",
    "test:integration:bond": "(NETWORK_FOLDER=../../../networks/local IDENTITY=REGISTRAR . ./bin/env-from-network && $(npm bin)/ts-mocha integration/oracle/bondIssuance.spec.ts)",
    "test:integration:emtn": "(NETWORK_FOLDER=../../../networks/local IDENTITY=REGISTRAR . ./bin/env-from-network && $(npm bin)/ts-mocha integration/oracle/emtnIssuance.spec.ts)",
    "test:integration:str": "(NETWORK_FOLDER=../../../networks/local IDENTITY=REGISTRAR . ./bin/env-from-network && $(npm bin)/ts-mocha integration/oracle/str.spec.ts)",
    "test:integration:servers": "(NETWORK_FOLDER=../../../networks/local IDENTITY=REGISTRAR . ./bin/env-from-network && $(npm bin)/ts-mocha integration/oracle/servers.spec.ts)",
    "test:integration:mocha": "(NETWORK_FOLDER=../../../networks/local IDENTITY=REGISTRAR . ./bin/env-from-network && $(npm bin)/ts-mocha integration/**/*.spec.ts)"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --ignore-path .gitignore --quiet"
  },
  "dependencies": {
    "@castframework/cast-interface-v1": "^0.8.3",
