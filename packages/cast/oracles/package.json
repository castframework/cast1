    "dd-trace": "^1.5.1",
    "delay": "^5.0.0",
    "env-cmd": "^10.1.0",
    "ethereumjs-util": "^7.1.3",
    "express": "4.17.1",
    "faker": "^5.5.3",
    "get-port": "^5.1.1",
    "graphql": "^15.8.0",
    "graphql-subscriptions": "^2.0.0",
