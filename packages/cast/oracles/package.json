    "@castframework/blockchain-driver-tz": "1.0.2-alpha.3",
    "@castframework/chain-auth": "^0.8.3",
    "@castframework/models": "^0.8.3",
    "@castframework/transaction-manager": "1.0.2-alpha.3",
